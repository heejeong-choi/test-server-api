<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.testserverapi.mapper.PrivateGroundMapper">

    <insert id="save" parameterType="com.example.testserverapi.model.dto.PrivateGroundReqDto">
        INSERT INTO private_ground
            (
             title,
             description,
             type,
             unit_amount,
             access_vehicle,
             space_size,
             theme,
             created_at,
             created_by,
             deleted
             )
         VALUES
            (
             #{title},
             #{description},
             #{privateGroundType.code},

            )
    </insert>
</mapper>